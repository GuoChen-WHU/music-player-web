{
  "name": "music-player-web",
  "version": "0.1.0",
  "description": "A music player running in browsers, build with react",
  "homepage": "https://guochen-whu.github.io/music-player-web",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node config/webpack.dev.js",
